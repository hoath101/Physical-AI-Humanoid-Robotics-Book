{
  "name": "physical-ai-humanoid-robotics-book",
  "version": "1.0.0",
  "description": "Educational book on Physical AI & Humanoid Robotics using Docusaurus",
  "main": "docusaurus.config.js",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "cross-env GIT_USER=hoath101 docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "api": "uv run main.py",
    "dev": "concurrently \"npm run api\" \"npm run start\" --kill-others-on-fail"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@openai/chatkit-react": "^1.3.0",
    "@types/styled-components": "^5.1.36",
    "axios": "^1.13.2",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "docusaurus-plugin-sass": "^0.2.6",
    "express": "^5.2.1",
    "gray-matter": "^4.0.3",
    "openai": "^6.10.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.96.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "gh-pages": "^6.3.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
